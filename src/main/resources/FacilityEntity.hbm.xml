<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fms.entity.FacilityEntity" table="Facility">
        <id name="id" column="id">
        <generator class="increment"/>
        </id>
        <property name="availableCapacity" column="availableCapacity"/>
        <property name="details" column="details"/>
        <property name="name" column="name"/>
        <property name="inUse" column="inUse"/>
        <property name="downTime" column="downTime"/>
    </class>
</hibernate-mapping>